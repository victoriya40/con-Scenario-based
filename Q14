import java.util.Scanner;
public class Main{
    public static void main(String[]args){
        Scanner sc=new Scanner(System.in);
        System.out.println("age: ");
        int a=sc.nextInt();
        if(a<18){
         System.out.println("child");
        }
        else if(a>18&&a<50){
            System.out.println("adult");
        }
        else{
            System.out.println("senior");
        }
        }
        }
    
