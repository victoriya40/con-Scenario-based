import java.util.Scanner;
public class Main{
    public static void main(String[]args){
        Scanner sc=new Scanner(System.in);
        System.out.println("age: ");
        int age=sc.nextInt();
        if(age>=60){
         System.out.println("eligible for citizen");
        }
        else{
            System.out.println("not eligible for citizen");
        }
        }
        }
    
